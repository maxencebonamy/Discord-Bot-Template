{
	"name": "discord-bot-template",
	"displayName": "Discord Bot Template",
	"version": "1.0.0",
	"scripts": {
		"dev": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts dev",
		"start": "node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts",
		"lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix",
		"test:unit": "vitest --ui",
		"ts:check": "tsc --noEmit",
		"create:command": "ts-node ./scripts/create-command/create-command.ts",
		"create:subcommand": "ts-node ./scripts/create-command/create-subcommand.ts",
		"create:task": "ts-node ./scripts/create-task/create-task.ts",
		"create:event": "ts-node ./scripts/create-event/create-event.ts"
	},
	"dependencies": {
		"cron": "^3.0.0",
		"dayjs": "^1.11.10",
		"discord.js": "^14.13.0",
		"rustic-error": "^0.2.1",
		"ts-node": "^10.9.2",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@maxencebonamy/eslint-config": "^1.0.2",
		"@swc-node/register": "^1.6.8",
		"@swc/core": "^1.3.91",
		"@types/node": "^20.10.5",
		"@types/yargs": "^17.0.32",
		"@vitest/ui": "^0.34.6",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
		"nodemon": "^3.0.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6",
		"yargs": "^17.7.2"
	},
	"eslintConfig": {
		"extends": "@maxencebonamy"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	}
}